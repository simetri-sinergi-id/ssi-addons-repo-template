{%- macro repo_id(slug) -%}
    {%- set repos_str -%}
        {%- include "vendor/maintainer-tools/tools/repos_with_ids.txt" -%}
    {%- endset -%}
    {%- for repo in repos_str.split("\n") -%}
        {%- if repo.endswith("/%s"|format(slug)) -%}
            {{- repo.split("|")[0] -}}
        {%- endif -%}
    {%- endfor -%}
{%- endmacro -%}
{%- macro odoo_version_slug(version) -%}
    {{- version | replace(".","-") -}}
{%- endmacro -%}
